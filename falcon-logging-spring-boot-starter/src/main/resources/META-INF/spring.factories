org.springframework.cloud.bootstrap.BootstrapConfiguration=\
    org.falconframework.logging.LoggingContext

org.springframework.boot.autoconfigure.EnableAutoConfiguration=\
    org.falconframework.logging.adapter.servlet.ServletAdapterAutoConfiguration,\
    org.falconframework.logging.adapter.rest.RestTemplateAdapterAutoConfiguration,\
    org.falconframework.logging.adapter.feign.FeignAdapterAutoConfiguration,\
    org.falconframework.logging.adapter.rabbit.RabbitAdapterAutoConfiguration,\
    org.falconframework.logging.aspect.ControllerAspectAutoConfiguration
