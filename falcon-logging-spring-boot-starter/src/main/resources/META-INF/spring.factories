org.springframework.context.ApplicationListener=\
  org.falcon.logging.adapter.application.ApplicationListenerAdapter

org.springframework.cloud.bootstrap.BootstrapConfiguration=\
    org.falcon.logging.config.ConfigReader

org.springframework.boot.autoconfigure.EnableAutoConfiguration=\
    org.falcon.logging.adapter.servlet.ServletAdapterAutoConfiguration,\
    org.falcon.logging.adapter.rest.RestTemplateAdapterAutoConfiguration,\
    org.falcon.logging.adapter.feign.FeignAdapterAutoConfiguration,\
    org.falcon.logging.adapter.rabbit.RabbitAdapterAutoConfiguration,\
    org.falcon.logging.printer.HttpRequestPrinterAutoConfiguration,\
    org.falcon.logging.config.ApolloConfigListenerAutoConfiguration
